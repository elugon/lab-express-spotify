<div>
    {{#each tracks}}
    <div>
        <figure class="audio-preview">
            <figcaption>{{this.name}}</figcaption>
            <audio controls src="{{preview_url}}">
                Your browser does not support the
                <code>audio</code> element.
            </audio>
        </figure>
    </div>
    {{/each}}
</div> 